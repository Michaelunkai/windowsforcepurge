sudo apt update && sudo apt install -y wget libnss3 zenity x11-utils && wget https://dist.torproject.org/torbrowser/14.5a2/tor-browser-linux-x86_64-14.5a2.tar.xz && tar -xvf tor-browser-linux-x86_64-14.5a2.tar.xz && cd tor-browser && chmod -R 755 ./Browser && chmod +x ./Browser/start-tor-browser ./Browser/start-tor-browser.desktop && sed -i 's/geteuid/getppid/' ./Browser/start-tor-browser && ./Browser/start-tor-browser --detach
