curl -LO https://github.com/google/go-containerregistry/releases/latest/download/go-containerregistry_Linux_x86_64.tar.gz && tar -xvzf go-containerregistry_Linux_x86_64.tar.gz && sudo mv crane /usr/local/bin/ && rm -f go-containerregistry_Linux_x86_64.tar.gz && crane version && wget https://github.com/ko-build/ko/releases/download/v0.17.1/ko_0.17.1_Linux_x86_64.tar.gz && tar -xvzf ko_0.17.1_Linux_x86_64.tar.gz && file ko && chmod +x ko && sudo mv ko /usr/local/bin/ && rm -f ko_0.17.1_Linux_x86_64.tar.gz && ko version
