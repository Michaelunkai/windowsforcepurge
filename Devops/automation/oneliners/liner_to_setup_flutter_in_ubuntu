cd && mkdir -p /root/flutter_project && cd /root/flutter_project && wget https://storage.googleapis.com/flutter_infra_release/releases/stable/linux/flutter_linux_3.10.6-stable.tar.xz && tar xf flutter_linux_3.10.6-stable.tar.xz && export PATH="/root/flutter_project/flutter/bin:$PATH" && git config --global --add safe.directory /root/flutter_project/flutter && flutter config --no-analytics && flutter doctor --android-licenses && flutter doctor && flutter --version
