Stop-Process -Name python -Force -ErrorAction SilentlyContinue; Start-Sleep -s 5; Remove-Item "$env:USERPROFILE\python-installer.exe" -Force -ErrorAction SilentlyContinue; Invoke-WebRequest -Uri https://www.python.org/ftp/python/3.12.4/python-3.12.4-amd64.exe -OutFile "$env:USERPROFILE\python-installer.exe"; Start-Process "$env:USERPROFILE\python-installer.exe" -ArgumentList '/quiet InstallAllUsers=1 PrependPath=1 Include_pip=1' -Wait; Remove-Item "$env:USERPROFILE\python-installer.exe" -Force -ErrorAction SilentlyContinue; python --version; pip --version
