curl -sSL https://packages.opennebula.io/repo/repo.key | sudo apt-key add - && echo "deb https://packages.opennebula.io/repo/6.0/Ubuntu $(lsb_release -cs) stable" | sudo tee /etc/apt/sources.list.d/opennebula.list && sudo apt install -y curl apt-transport-https lsb-release gnupg opennebula-tools && mkdir -p ~/.one && echo "oneadmin:admin_password" > ~/.one/one_auth && export ONE_AUTH="/home/ubuntu/.one/one_auth" && export ONE_XMLRPC="http://192.168.1.100:2633/RPC2" && oneuser show && onevm list
