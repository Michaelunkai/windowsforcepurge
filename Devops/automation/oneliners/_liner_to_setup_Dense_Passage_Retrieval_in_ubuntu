mkdir dpr && cd dpr && python3 -m venv venv && source venv/bin/activate && pip install transformers && git clone https://github.com/facebookresearch/DPR.git && cd DPR && pip install torch torchvision torchaudio --extra-index-url https://download.pytorch.org/whl/cu111 && python3 -c "import torch; print(torch.version)" && python -c 'from transformers import DPRQuestionEncoder; model = DPRQuestionEncoder.from_pretrained("facebook/dpr-question_encoder-single-nq-base"); print(model)'
