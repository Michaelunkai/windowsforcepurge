Set-ExecutionPolicy RemoteSigned -Scope CurrentUser; [System.Net.ServicePointManager]::SecurityProtocol = [System.Net.ServicePointManager]::SecurityProtocol -bor 3072; Invoke-WebRequest -Uri "https://dotnet.microsoft.com/download/dotnet/scripts/v1/dotnet-install.ps1" -OutFile "$env:USERPROFILE\dotnet-install.ps1"; & "$env:USERPROFILE\dotnet-install.ps1" -Channel Current; [Environment]::SetEnvironmentVariable("PATH", $env:PATH + ";$env:USERPROFILE\.dotnet", [System.EnvironmentVariableTarget]::User); dotnet --list-sdks
