update && sudo apt install gnupg curl -y && curl -fsSL https://www.mongodb.org/static/pgp/server-7.0.asc |    sudo gpg -o /usr/share/keyrings/mongodb-server-7.0.gpg    --dearmor && echo "deb [ arch=amd64,arm64 signed-by=/usr/share/keyrings/mongodb-server-7.0.gpg ] https://repo.mongodb.org/apt/ubuntu jammy/mongodb-org/7.0 multiverse" | sudo tee /etc/apt/sources.list.d/mongodb-org-7.0.list && update && cc && sudo apt-get install -y mongodb-org -y && sudo systemctl start mongod && sudo systemctl daemon-reload &&  sudo systemctl enable mongod && cc && sudo systemctl status mongod && mongosh
