Invoke-WebRequest -Uri "https://s3.amazonaws.com/session-manager-downloads/plugin/latest/windows/SessionManagerPluginSetup.exe" -OutFile "SessionManagerPluginSetup.exe"; Start-Process -FilePath .\SessionManagerPluginSetup.exe -ArgumentList '/quiet' -Wait; Remove-Item -Path .\SessionManagerPluginSetup.exe; $env:Path += ";C:\Program Files\Amazon\SessionManagerPlugin\bin"; [Environment]::SetEnvironmentVariable("Path", $env:Path, [EnvironmentVariableTarget]::Machine); session-manager-plugin --version
