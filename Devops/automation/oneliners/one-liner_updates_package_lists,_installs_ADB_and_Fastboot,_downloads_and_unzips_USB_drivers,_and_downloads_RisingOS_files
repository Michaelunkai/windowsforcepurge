updates && sudo apt install adb fastboot -y && cd && wget https://dl.google.com/android/repository/usb_driver_r13-windows.zip -O usb_driver_r13-windows.zip && unzip usb_driver_r13-windows.zip -d usb_driver && mkdir risingos && cd risingos && wget https://sourceforge.net/projects/risingos-for-bluejay/files/latest/download -O rom.zip && wget https://sourceforge.net/projects/risingos-for-bluejay/files/boot.img -O boot.img && wget https://sourceforge.net/projects/risingos-for-bluejay/files/dtbo.img -O dtbo.img && wget https://sourceforge.net/projects/risingos-for-bluejay/files/vendor_boot.img -O vendor_boot.img
