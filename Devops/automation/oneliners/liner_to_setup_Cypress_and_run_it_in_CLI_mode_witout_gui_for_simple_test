getnpm && cd && mkdir cypress-test && cd cypress-test && npm init -y && npm install cypress --save-dev && sudo apt install libnss3 libnspr4 libxss1 libasound2 libatk1.0-0 libatk-bridge2.0-0 libcups2 libdrm2 libdbus-1-3 libxkbcommon0 libgbm1 libgtk-3-0 -y && echo 'module.exports = { e2e: { supportFile: "cypress/support/e2e.js" } };' > cypress.config.js && mkdir -p cypress/support cypress/e2e && echo '// support file' > cypress/support/e2e.js && echo 'describe("My First Test", () => { it("Visits Google", () => { cy.visit("https://www.google.com") }) })' > cypress/e2e/test.cy.js && npx cypress run
