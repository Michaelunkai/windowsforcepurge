 Invoke-WebRequest -Uri "https://download.visualstudio.microsoft.com/download/pr/b08c26a7-83de-4d9a-b4d7-d87942b6d35e/d6d973d0cfe914189692af1154690935/dotnet-sdk-9.0.100-preview.5.24307.3-win-x64.zip" -OutFile "$env:TEMP\dotnet-sdk.zip"; Expand-Archive -Path "$env:TEMP\dotnet-sdk.zip" -DestinationPath "$env:ProgramFiles\dotnet"; Remove-Item "$env:TEMP\dotnet-sdk.zip"; & "$env:ProgramFiles\dotnet\dotnet.exe" --version
