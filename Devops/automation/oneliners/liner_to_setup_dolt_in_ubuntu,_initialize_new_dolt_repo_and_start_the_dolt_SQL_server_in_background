 sudo apt update && sudo apt install -y curl unzip && curl -LO https://github.com/dolthub/dolt/releases/download/v1.43.0/dolt-linux-amd64.tar.gz && tar -xzf dolt-linux-amd64.tar.gz && sudo mv dolt-linux-amd64/bin/dolt /usr/local/bin/ && rm dolt-linux-amd64.tar.gz && dolt version && mkdir my_dolt_repo && cd my_dolt_repo &&   dolt config --global --add user.email "you@example.com" && dolt config --global --add user.name "Your Name" && dolt init && dolt sql-server
