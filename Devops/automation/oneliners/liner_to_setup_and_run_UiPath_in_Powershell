$uipathInstallerUrl = "https://download.uipath.com/UiPathStudioSetup.exe"; $installerPath = "$env:TEMP\UiPathStudioSetup.exe"; Invoke-WebRequest -Uri $uipathInstallerUrl -OutFile $installerPath; Start-Process -FilePath $installerPath -ArgumentList "/Silent /Install /Log $env:TEMP\UiPathInstall.log" -Wait; $studioPath = "C:\Program Files (x86)\UiPath\Studio\UiPath.Studio.exe"; if (Test-Path $studioPath) { Start-Process -FilePath $studioPath }
