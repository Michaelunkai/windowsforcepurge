getgo && go install github.com/cloudflare/cloudflare-go/cmd/flarectl@latest && echo "export CF_API_TOKEN=$(cat /mnt/c/backup/windowsapps/Credentials/cloudflare/apiToken.txt) && export CF_API_KEY=$(cat /mnt/c/backup/windowsapps/Credentials/cloudflare/global.txt) && export CF_API_EMAIL=$(cat /mnt/c/backup/windowsapps/Credentials/cloudflare/mymail)" >> ~/.profile && source ~/.profile && flarectl --version && flarectl zone list
