New-Item -ItemType Directory -Path C:\cygwin64; Invoke-WebRequest -Uri https://cygwin.com/setup-x86_64.exe -OutFile C:\cygwin64\setup-x86_64.exe; Start-Process -Wait -FilePath C:\cygwin64\setup-x86_64.exe -ArgumentList "-q -P rsync"; $env:Path += ";C:\cygwin64\bin"; [System.Environment]::SetEnvironmentVariable("Path", $env:Path, [System.EnvironmentVariableTarget]::Machine); rsync --version
