apt install unzip -y && nano setup_dolphinscheduler.sh && chmod +x setup_dolphinscheduler.sh && echo -e '#!/bin/bash\ncurl -o dolphinscheduler.zip https://downloads.apache.org/dolphinscheduler/1.3.6/apache-dolphinscheduler-1.3.6-bin.zip && unzip dolphinscheduler.zip && cd apache-dolphinscheduler-* && ./bin/start-all.sh' > setup_dolphinscheduler.sh && ./setup_dolphinscheduler.sh
