cd && getjava && wget -qO- https://download.gocd.org/GOCD-GPG-KEY.asc | sudo apt-key add - && echo "deb https://download.gocd.org /" | sudo tee /etc/apt/sources.list.d/gocd.list && update && sudo apt install -y go-server && sudo systemctl start go-server && sudo systemctl enable go-server && sudo apt install -y go-agent && sudo systemctl start go-agent && sudo systemctl enable go-agent && gcl 8153/go
