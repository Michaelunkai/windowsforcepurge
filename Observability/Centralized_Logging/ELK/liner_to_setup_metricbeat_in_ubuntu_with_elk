 getelk && sudo apt-get install -y apt-transport-https && wget -qO - https://artifacts.elastic.co/GPG-KEY-elasticsearch | sudo apt-key add - && sudo sh -c 'echo "deb https://artifacts.elastic.co/packages/7.x/apt stable main" > /etc/apt/sources.list.d/elastic-7.x.list' && sudo apt-get update && sudo apt-get install -y metricbeat && sudo metricbeat modules enable system && sudo metricbeat setup --dashboards && sudo systemctl enable metricbeat && sudo systemctl start metricbeat && sudo systemctl status metricbeat
