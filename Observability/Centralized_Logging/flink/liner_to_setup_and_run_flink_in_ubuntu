sudo apt-get install -y openjdk-11-jdk wget && wget https://archive.apache.org/dist/flink/flink-1.13.6/flink-1.13.6-bin-scala_2.11.tgz && tar -xvzf flink-1.13.6-bin-scala_2.11.tgz && cd flink-1.13.6 && echo "env.java.opts: --add-opens java.base/sun.net.util=ALL-UNNAMED" >> conf/flink-conf.yaml && ./bin/start-cluster.sh && cc && jps | grep -E 'StandaloneSessionClusterEntrypoint|TaskManagerRunner' && gc http://localhost:8081
