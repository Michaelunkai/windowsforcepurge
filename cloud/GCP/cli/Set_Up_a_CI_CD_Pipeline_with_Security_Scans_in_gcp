gcloud builds submit --tag gcr.io/my-project/my-app && gcloud container images add-tag gcr.io/my-project/my-app gcr.io/my-project/my-app:latest && gcloud beta container analysis notes create --project=my-project --occurrence-filter='resourceUrl=gcr.io/my-project/my-app@sha256:*' --note-id=my-vuln-scan --short-description='Vulnerability scan' --description='A vulnerability scan note for my app'
