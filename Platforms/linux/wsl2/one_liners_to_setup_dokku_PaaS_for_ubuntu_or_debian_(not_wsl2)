wget -NP . https://dokku.com/install/v0.34.4/bootstrap.sh && sudo DOKKU_TAG=v0.34.4 bash bootstrap.sh && sudo apt-get install -f --fix-broken && sudo systemctl restart nginx && cat ~/.ssh/authorized_keys | dokku ssh-keys:add admin && dokku domains:set-global dokku.me && dokku domains:set-global 10.0.0.2 && dokku domains:set-global 10.0.0.2.sslip.io && dokku apps:create myapp && git remote add dokku dokku@10.0.0.2:myapp && git push dokku master
