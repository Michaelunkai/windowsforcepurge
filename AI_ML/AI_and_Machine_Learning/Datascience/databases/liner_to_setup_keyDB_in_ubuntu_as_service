echo "deb https://download.keydb.dev/open-source-dist $(lsb_release -sc) main" | sudo tee /etc/apt/sources.list.d/keydb.list && sudo wget -O /etc/apt/trusted.gpg.d/keydb.gpg https://download.keydb.dev/open-source-dist/keyring.gpg && sudo apt update && sudo apt --fix-broken install -y && sudo apt install keydb-server keydb -y && sudo systemctl enable keydb-server.service && sudo systemctl start keydb-server.service && sudo systemctl status keydb-server.service
